[user]
	name = Patrick Mooney
	email = patrick.f.mooney@gmail.com
[color]
	ui = auto

[push]
	default = simple
[alias]
	last = !sh -c "'if [ \"$TMUX\" ]; then tmux set-buffer $(git log -n1 --format='%H'); git log -n1; fi'"
	pr = !sh -c 'git fetch origin "pull/$1/head:pr$1"' -
	up = pull --ff-only
	ds = diff --stat
	st = status --short
